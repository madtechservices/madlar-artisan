{
    "name": "tomatophp/tomato-artisan",
    "type": "library",
    "description": "artisan terminal to run artisan commands using GUI",
    "keywords": [
        "php",
        "laravel",
        "template",
        "artisan",
        "config",
        "commands",
        "tomato"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoArtisan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoArtisan\\TomatoArtisanServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0",
        "tomatophp/tomato-admin": "^1.0",
        "tomatophp/tomato-roles": "^1.0"
    }
}
